ом? "+(n?"За"+L.g+"подробный"+L.g+"отзыв — баллы Плюса":""),pro:a,contra:i,comment:u}}))(nr),ar=n(2032),or=n(8807),ir=n(3394),cr=n.n(ir),ur=function(e){var t=e.id,n=e.title,r=void 0===n?"":n,a=e.options,o=e.value,i=void 0===o?"":o,c=e.dataAuto,u=e.onChange,s=Object(v.useState)(String(i)),d=s[0],l=s[1],p=Object(v.useCallback)((function(e){l(e.target.value),u({value:e.target.value})}),[u,l]);return g.a.createElement(Yn,{dataAuto:c,className:cr.a.root,titleClassName:cr.a.title,title:r,"data-tid":"9d008456"+(e&&e["data-tid-prop"]?" "+e["data-tid-prop"]:""),"data-tid-prop":"9d008456"+(e&&e["data-tid-prop"]?" "+e["data-tid-prop"]:"")},a.map((function(e){var n=e.value,r=e.name,a=String(n);return g.a.createElement(or.b,{key:a,id:t+"_"+a,name:r,value:a,checked:a===d,onChange:p,theme:"review",size:"m"},g.a.createElement(B.a,{size:"350"},r))})))},sr=g.a.memo(ur),dr=Object.keys(en.x).map((function(e){return{value:String(e),name:Object(ar.a)(en.x[e])}})),lr=Object(y.connect)((function(e,t){var n=t.usage;return{id:"usage",title:en.t,value:null!=n&&String(n),options:dr,dataAuto:"review-usage"}}))(sr),pr=n(2278),fr=n(3396),mr=0,hr=1,br=n(3395),vr=n.n(br),gr=function(e){var t=e.className,n=e.title,r=e.description,a=e.value,o=e.onChange,i=Object(v.useState)(a),c=i[0],u=i[1],s=Object(v.useCallback)((function(e){u(e),o({value:e})}),[o]);return g.a.createElement("div",{className:ct()(t,vr.a.root),"data-tid":"57475a36"+(e&&e["data-tid-prop"]?" "+e["data-tid-prop"]:"")},g.a.createElement(B.a,{className:vr.a.title},n),r&&g.a.createElement("div",{className:vr.a.description},r),g.a.createElement(U.a,{className:vr.a.ratingInput,value:c,size:"xl2",theme:D.a.ORANGE,onChange:s}))},_r=Object(kt.compose)(Object(y.connect)((function(e,t){var n=t.factorsMap,r=t.factorId,a=Object(fr.a)(e,{factorId:r}),o=a.radioValues,i=a.title,c=a.description,u=a.type,s=(n[r]||{}).value;return{id:r,title:i,description:c,value:void 0===s?null:s,options:o,type:u}}),kt.identity,(function(e,t,n){return ce()({},e,n,{onCha
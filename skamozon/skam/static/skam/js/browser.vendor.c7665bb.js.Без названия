ики",d[T]="Характеристика не соответствует",d[N]="Магазин накручивает отзывы",d[A]="Магазин является клоном",d[D]="Мошенничество",d[P]="Запрещенный товар",d[b]="Продажа товаров, бывших в употреблении",d[v]="Продажа поддельной продукции",d[R]="Неправильные юридические данные",d[C]="Небезопасный и оскорбительный товар",(l={})[A]="Сходные магазины (аффилиаты)",l[P]="Продажа запрещённых товаров",l[D]="Подозрение в мошенничестве",(p={})[R]="MARKETQUALITY",p[N]="MARKETREVIEWS",p[A]="MARKETCL",p[v]="MARKETQUALITY",p[D]="MARKETQUALITY",p[b]="MARKETQUALITY",p[j]="MARKETQUALITY",p[C]="BQ",p[P]="MARKETQUALITY",p)},function(e,t,n){e.exports={issuesList:"_2V06L",issueWrapper:"_2ZyS3"}},function(e,t,n){e.exports={root:"XOMIb",titleWrapper:"arXrq",title:"_1-id8",description:"_1h4Ig"}},function(e,t,n){e.exports={causesList:"_3jP4d",causeButtons:"_3Zs_4",causeButton:"UT07R",causeButtonWithMargin:"_1-J09",causeBackButton:"_3BT1u"}},function(e,t,n){e.exports={root:"cUeQO"}},function(e,t,n){e.exports={textField:"_3y4C1",formValidationMessage:"_35rsC",complainButtons:"_2wKd2",complainBackButton:"_3eMbT",complainButton:"_1Wa3F"}},function(e,t,n){"use strict";n.d(t,"b",(function(){return r})),n.d(t,"c",(function(){return a})),n.d(t,"d",(function(){return o})),n.d(t,"a",(function(){return i}));var r="complainTypeOfferId",a="complainTypeProduct",o="complainTypeShopId",i="complainFormPopup"},function(e,t,n){e.exports={root:"_1rFcG"}},function(e,t){},function(e,t,n){"use strict";var r=n(1363);e.exports=(r.default||r)("src/resolvers/startrek/createTicketFromComplaint.js",[{name:"createTicketFromComplaint",bulk:"default"}],{})},function(e,t,n){},,,,,,,function(e,t,n){"use strict";n.d(t,"b",(function(){return r})),n.d(t,"a",(function(){return a}));var r={BOOK:"book",CLUSTER:"cluster",MODEL:"model",MODIFICATION:"modification",GROUP:"group"},a={"market:product":"PRODUCT_BUNDLE_MAIN","market:product-offers":"PRODUCT_BUNDLE_OFFERS","market:product-articles":"PRODUCT_BUNDLE_ARTICLES","market:product-